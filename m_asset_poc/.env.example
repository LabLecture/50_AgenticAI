# 데이터베이스 설정
DB_USER=postgres
DB_PASSWORD=postgres
DB_HOST=localhost
DB_PORT=5432
DB_NAME=postgres

# Ollama 설정 (임베딩 생성용)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_EMBEDDING_MODEL=bge-m3:latest

# VLLM 설정 (SQL 생성용 LLM)
VLLM_SERVER_URL=http://192.168.1.239/vllm9/v1
VLLM_MODEL_NAME=qwencoder-32b
VLLM_API_KEY=EMPTY
VLLM_TEMPERATURE=0

# Weaviate 설정 (벡터 스토어)
WEAVIATE_HOST=localhost
WEAVIATE_PORT=8080
WEAVIATE_SCHEMA_COLLECTION=M_asset_hybrid_1
WEAVIATE_SAMPLE_COLLECTION=M_asset_sample_query_2_hint
WEAVIATE_ENABLED=true

# 벡터 스토어 연결 풀 설정
VECTOR_STORE_USE_POOL=true           # 연결 풀 사용 여부
VECTOR_STORE_MIN_CONNECTIONS=2       # 최소 연결 수
VECTOR_STORE_MAX_CONNECTIONS=10      # 최대 연결 수
VECTOR_STORE_CONNECTION_LIFETIME=3600 # 연결 수명 (초)
VECTOR_STORE_HEALTH_CHECK_INTERVAL=60 # 헬스 체크 간격 (초)

# Langfuse 설정 (추적 시스템)
LANGFUSE_PUBLIC_KEY=pk-lf-ca041d2f-740e-471d-8b63-bf3c94230c07
LANGFUSE_SECRET_KEY=sk-lf-c59f4349-7bc6-41ed-a292-a2b11696c30a
LANGFUSE_HOST=http://localhost:3000
LANGFUSE_ENABLED=true

# API 설정
API_HOST=0.0.0.0
API_PORT=8010
API_DEBUG=false

# 기타 설정
MAX_TEXT_TO_SQL_RETRIES=3
VECTOR_STORE_OPTIONAL=true  # 벡터 스토어 없이도 실행 가능

# 로그 설정
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_DIR=logs
LOG_CONSOLE=true  # 콘솔 출력 여부
LOG_FILE=true     # 파일 출력 여부

# A2A (Agent-to-Agent) Protocol 설정
A2A_ENABLED=true                                                            # A2A 기능 활성화
A2A_BASE_URL=http://localhost:8010                                         # A2A 에이전트 기본 URL
A2A_AUTH_TOKEN=                                                             # A2A 인증 토큰 (선택사항)
A2A_AGENT_NAME=Market Agent                                                 # 에이전트 이름
A2A_AGENT_DESCRIPTION=한국 증권 데이터에 대한 자연어 질의를 SQL로 변환하고 실행하는 에이전트  # 에이전트 설명
A2A_AGENT_VERSION=1.0.0                                                    # 에이전트 버전
A2A_STREAMING_ENABLED=true                                                 # SSE 스트리밍 지원
A2A_PUSH_NOTIFICATIONS_ENABLED=false                                       # 푸시 알림 지원
A2A_STATE_HISTORY_ENABLED=true                                             # 상태 히스토리 지원

# LLM 캐싱 설정
LLM_CACHE_ENABLED=true              # LLM 응답 캐싱 활성화
LLM_CACHE_TTL=3600                  # 캐시 TTL (초, 기본 1시간)
LLM_CACHE_MAX_SIZE=10000            # 최대 캐시 항목 수
REDIS_HOST=localhost                # Redis 호스트
REDIS_PORT=6379                     # Redis 포트
REDIS_DB=0                          # Redis DB 번호
REDIS_PASSWORD=                     # Redis 비밀번호 (선택사항)

# 동시성 제어 설정
MAX_CONCURRENT_REQUESTS=10          # API 서버 최대 동시 요청 수
A2A_MAX_CONCURRENT_REQUESTS=10      # A2A 서버 최대 동시 요청 수
REQUEST_QUEUE_TIMEOUT=300           # 대기열 타임아웃 (초)

# Excel 배치 처리 설정
EXCEL_MAX_CONCURRENT_QUERIES=5      # Excel 처리 최대 동시 쿼리 수
EXCEL_QUERY_TIMEOUT=300             # Excel 쿼리 타임아웃 (초)